<%- include("../partials/header") -%>
        <h3> Speakers List </h3>
        <table class="table table-dark">
                <thead>
                  <tr>
                    <th scope="col">#</th>
                    <th scope="col">name</th>
                    <th scope="col">age</th>
                    <th scope="col">User Name</th>
                    <th scope="col">User Password</th>
                  </tr>
                </thead>
                <tbody>
                 <%speakers.forEach((speaker)=>{%>
                      <tr>
                        <td><%=speaker._id%></td>
                        <td><%=speaker.name%></td>
                        <td><%=speaker.age%></td>
                        <td><%=speaker.username%></td>
                        <td><%=speaker.userpass%></td>
                        <td><a href="/speakers/edit/<%=speaker._id%>" class="btn btn-primary">EDIT</a></td>
                        <td><a href="/speakers/delete/<%=speaker._id%>" class="btn btn-danger">DELETE</a></td>
                       
                 <%})%>
                </tbody>
              </table>